$("#dimensions").html("");
<% @dimensions.each do |d| %>
  $("#dimensions").append($(Mustache.to_html($('#dimension_template').html(), <%= d.to_json(:url => user_dimension_path(current_user, d)).to_s.html_safe %>)));
<% end %> 
$('#dimension-paginator').html('<%= escape_javascript(paginate(@dimensions, 
                                                               :params => {:controller => 'dimensions', :action => 'index'}, 
                                                               :remote => true).to_s) %>');  
set_icons_tooltips();                                                                                                                     